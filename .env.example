# TrenchCoat Pro Environment Variables
# Copy this file to .env and fill in your actual values

# GitHub Authentication
GITHUB_TOKEN=your_github_token_here

# Discord Webhooks  
DISCORD_WEBHOOK_DEV=your_dev_webhook_url
DISCORD_WEBHOOK_DEPLOY=your_deployment_webhook_url
DISCORD_WEBHOOK_OVERVIEW=your_overview_webhook_url

# API Keys (if needed)
COINGECKO_API_KEY=your_coingecko_key
SOLSCAN_API_KEY=your_solscan_key

# Database Configuration
DATABASE_PATH=data/trench.db